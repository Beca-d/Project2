<ol class="">
    {{#each posts as |post|}}  
    <li>
       <article class="">
            <div class="">
                <div class="">
                    <h1>Email {{email}}</h1>
                    <span>Username <b>{{username}}</b></span>
                    <img src="{{image}}" width="10%" height="20%" class="profile-avatar">
                    <form>
                        <label for="password-login">Password:</label>
                        <input type="file" id="password-login" />
                    </form>
                </div>
            </div>
            <div>
                <h2>id {{id}}</h2>
            </div>
        </article>
    </li>
    {{/each}}
</ol>

    